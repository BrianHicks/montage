---
source: montage_client/src/report.rs
expression: operation.query
---
query ReportQuery($start: DateTime!, $end: DateTime!) {
  report(end: $end, start: $start) {
    start
    end
    totals {
      shortBreak
      longBreak
      task
      meeting
      working
      tasksByDescription {
        description
        total
      }
    }
    sessions {
      description
      actualDuration
      kind
      startTime
    }
  }
}


